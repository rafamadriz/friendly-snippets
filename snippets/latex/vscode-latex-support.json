    "Table": {
        "prefix": "table",
        "body": [
            "${1:Table}~\\ref{${2:tab:}}$0"
        ],
        "description": "Table"
    },
    "Tabular": {
        "prefix": "tab",
        "body": [
            "\\begin{table}[${1:htbp}]",
            "\t\\centering\\begin{tabular}{${4:<columns>}}",
            "\t\t$0",
            "\t\\end{tabular}",
            "\t\\caption{${2:<caption>}}",
            "\t\\label{${3:<label>}}",
            "\\end{table}"
        ],
        "description": "Tabular"
    },
    "\\begin{}â€¦\\end{}": {
        "prefix": "begin",
        "body": [
            "\\\\begin{${1:env}}",
            "\t$1",
            "\\\\end{${1:env}}"
        ],
        "description": "Begin - End"
    }
}
