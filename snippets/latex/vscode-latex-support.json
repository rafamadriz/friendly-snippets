            "\\\\end{itemize}"
        ],
        "description": "Itemize"
    },
    "Listing": {
        "prefix": "listing",
        "body": [
            "${1:Listing}~\\ref{${2:lst:}}$0"
        ],
        "description": "Listing"
    },
    "Matrix": {
        "prefix": "mat",
        "body": [
            "\\begin{${1:p/b/v/V/B/small}matrix}",
            "\t$0",
            "\\end{${1:p/b/v/V/B/small}matrix}"
        ],
        "description": "Matrix"
    },
    "Page": {
        "prefix": "page",
        "body": [
            "${1:page}~\\pageref{$2}$0"
        ],
        "description": "Page"
    },
    "Paragraph": {
