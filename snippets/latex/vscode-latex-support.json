    "Region Start": {
        "prefix": "#region",
        "body": [
            "%#Region $0"
        ],
        "description": "Folding Region Start"
    },
    "Region End": {
        "prefix": "#endregion",
        "body": [
            "%#Endregion"
        ],
        "description": "Folding Region End"
    },
    "Section": {
        "prefix": "section",
        "body": [
            "${1:Section}~\\ref{${2:sec:}}$0"
        ],
        "description": ""
    },
    "Split": {
