{
    "Align(ed)": {
        "prefix": "ali",
        "body": [
            "\\begin{align}",
            "\t$0",
            "\\end{align}"
        ],
        "description": "Align(ed)"
    },
    "Cases": {
        "prefix": "cas",
        "body": [
            "\\begin{cases}",
            "\t${1:equation}, &\\text{ if }${2:case}\\\\\\\\",
            "\t$0",
            "\\end{cases}"
        ],
        "description": "Cases"
    },
    "Chapter": {
        "prefix": "cha",
        "body": [
            "\\chapter{${1:chapter name}} % (fold)",
            "\\label{cha:${2:${1/(\\w+)(\\W+$)?|\\W+/${1:?${1:/asciify/downcase}:_}/g}}}",
            "${0:$TM_SELECTED_TEXT}",
            "% chapter $2 (end)"
        ],
        "description": "Chapter"
    },
    "Description": {
        "prefix": "desc",
        "body": [
            "\\\\begin{description}",
            "\t\\item[$1] $0",
            "\\\\end{description}"
        ],
        "description": "Description"
    },
    "Enumerate": {
        "prefix": "enum",
        "body": [
            "\\\\begin{enumerate}",
            "\t\\item $0",
            "\\\\end{enumerate}"
        ],
        "description": "Enumerate"
    },
    "Equation": {
        "prefix": "eq",
        "body": [
            "\\begin{equation}",
            "\t$0",
            "\\end{equation}"
        ],
        "description": "Equation"
    },
    "Figure": {
        "prefix": "figure",
        "body": [
            "${1:Figure}~\\ref{${2:fig:}}$0"
        ],
        "description": "Figure"
    },
    "Gather(ed)": {
        "prefix": "gat",
        "body": [
            "\\begin{gather}",
            "\t$0",
            "\\end{gather}"
        ],
        "description": "Gather(ed)"
    },
    "Itemize": {
        "prefix": "item",
        "body": [
            "\\\\begin{itemize}",
