            "    chapter = {${11:?_chapter}},",
            "    pages = {${12:?_pages}},",
            "    address = {${13:?_address}},",
            "    edition = {${14:?_edition}},",
            "    year = {${15:year}},",
            "    month = {${16:?_month}},",
            "    note = {${17:?_note}},",
            "}"
