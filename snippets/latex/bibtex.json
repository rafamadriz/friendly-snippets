{
    "@article": {
        "prefix": "@article",
        "description": "An article from a magazine or a journal.",
        "body": [
            "@article{${1:name},",
            "    author = {${2:author}},",
            "    title = {${3:title}},",
            "    journal = {${4:journal}},",
            "    volume = {${5:?_volume}},",
            "    number = {${6:?_number}},",
            "    pages = {${7:?_pages}},",
            "    year = {${8:year}},",
            "    month = {${9:?_month}},",
            "    note = {${10:?_note}},",
            "}"
        ]
    },
    "@book": {
        "prefix": "@book",
        "description": "A published book",
        "body": [
            "@book{${1:name},",
            "    author = {${2:author}},",
            "    title = {${3:title}},",
            "    publisher = {${4:publisher}},",
            "    volume = {${5:?_volume}},",
            "    number = {${6:?_number}},",
            "    series = {${7:?_series}},",
            "    address = {${8:?_address}},",
            "    edition = {${9:?_edition}},",
            "    year = {${10:year}},",
            "    month = {${11:?_month}},",
            "    note = {${12:?_note}},",
            "}"
        ]
    },
    "@booklet": {
        "prefix": "@booklet",
        "description": "A bound work without a named publisher or sponsor.",
        "body": [
            "@booklet{${1:name},",
            "    author = {${1:?_author}},",
            "    title = {${2:title}},",
            "    howpublished = {${3:?_howpublished}},",
            "    address = {${4:?_address}},",
            "    year = {${5:?_year}},",
            "    month = {${6:?_month}},",
            "    note = {${7:?_note}},",
            "}"
        ]
    },
    "@conference": {
        "prefix": "@conference",
        "description": "Equal to inproceedings",
        "body": [
            "@conference{${1:name},",
            "    author = {${2:author}},",
            "    title = {${3:title}},",
            "    booktitle = {${4:booktitle}},",
            "    editor = {${5:?_editor}},",
            "    volume = {${6:?_volume}},",
            "    number = {${7:?_number}},",
            "    series = {${8:?_series}},",
            "    pages = {${9:?_pages}},",
            "    address = {${10:?_address}},",
            "    year = {${11:year}},",
            "    month = {${12:?_month}},",
            "    publisher = {${13:?_publisher}},",
            "    note = {${14:?_note}},",
            "}"
        ]
    },
    "@inbook": {
        "prefix": "@inbook",
        "description": "A section of a book without its own title.",
        "body": [
            "@inbook{${1:name},",
            "    author = {${2:author}},",
            "    editor = {${3:editor}},",
            "    title = {${4:title}},",
            "    chapter = {${5:chapter}},",
            "    pages = {${6:pages}},",
            "    publisher = {${7:publisher}},",
            "    volume = {${8:?_volume}},",
            "    number = {${9:?_number}},",
            "    series = {${10:?_series}},",
            "    type = {${11:?_type}},",
            "    address = {${12:?_address}},",
            "    edition = {${13:?_edition}},",
            "    year = {${14:year}},",
            "    month = {${15:?_month}},",
            "    note = {${16:?_note}},",
            "}"
        ]
    },
    "@incollection": {
        "prefix": "@incollection",
        "description": "A section of a book having its own title.",
        "body": [
            "@incollection{${1:name},",
            "    author = {${2:author}},",
            "    title = {${3:title}},",
            "    booktitle = {${4:booktitle}},",
