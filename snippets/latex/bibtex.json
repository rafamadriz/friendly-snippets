{
    "@article": {
        "prefix": "@article",
        "description": "An article from a magazine or a journal.",
        "body": [
            "@article{Xarticle,",
            "    author = {${1:author}},",
            "    title = {${2:title}},",
            "    journal = {${3:journal}},",
            "    volume = {${4:?_volume}},",
            "    number = {${5:?_number}},",
            "    pages = {${6:?_pages}},",
            "    year = {${7:year}},",
            "    month = {${8:?_month}},",
            "    note = {${9:?_note}},",
            "}"
        ]
    },
    "@book": {
        "prefix": "@book",
        "description": "A published book",
        "body": [
            "@book{Xbook,",
            "    author = {${1:author}},",
            "    title = {${2:title}},",
            "    publisher = {${3:publisher}},",
            "    volume = {${4:?_volume}},",
            "    number = {${5:?_number}},",
            "    series = {${6:?_series}},",
            "    address = {${7:?_address}},",
            "    edition = {${8:?_edition}},",
            "    year = {${9:year}},",
            "    month = {${10:?_month}},",
            "    note = {${11:?_note}},",
            "}"
        ]
    },
    "@booklet": {
        "prefix": "@booklet",
        "description": "A bound work without a named publisher or sponsor.",
        "body": [
            "@booklet{Xbooklet,",
            "    author = {${0:?_author}},",
            "    title = {${1:title}},",
            "    howpublished = {${2:?_howpublished}},",
            "    address = {${3:?_address}},",
            "    year = {${4:?_year}},",
            "    month = {${5:?_month}},",
            "    note = {${6:?_note}},",
            "}"
        ]
    },
    "@conference": {
        "prefix": "@conference",
        "description": "Equal to inproceedings",
        "body": [
            "@conference{Xconference,",
            "    author = {${1:author}},",
            "    title = {${2:title}},",
            "    booktitle = {${3:booktitle}},",
            "    editor = {${4:?_editor}},",
            "    volume = {${5:?_volume}},",
            "    number = {${6:?_number}},",
            "    series = {${7:?_series}},",
            "    pages = {${8:?_pages}},",
            "    address = {${9:?_address}},",
            "    year = {${10:year}},",
            "    month = {${11:?_month}},",
            "    ?_publisher = {${12:?_publisher}},",
            "    note = {${13:?_note}},",
            "}"
        ]
    },
    "@inbook": {
        "prefix": "@inbook",
        "description": "A section of a book without its own title.",
        "body": [
            "@inbook{Xinbook,",
            "    author = {${1:author}},",
            "    editor = {${2:editor}},",
            "    title = {${3:title}},",
            "    chapter = {${4:chapter}},",
            "    pages = {${5:pages}},",
            "    publisher = {${6:publisher}},",
            "    volume = {${7:?_volume}},",
            "    number = {${8:?_number}},",
            "    series = {${9:?_series}},",
            "    type = {${10:?_type}},",
            "    ?_address = {${11:?_address}},",
            "    ?_edition = {${12:?_edition}},",
            "    year = {${13:year}},",
            "    month = {${14:?_month}},",
            "    note = {${15:?_note}},",
            "}"
        ]
    },
    "@incollection": {
        "prefix": "@incollection",
        "description": "A section of a book having its own title.",
        "body": [
            "@incollection{Xincollection,",
            "    author = {${1:author}},",
            "    title = {${2:title}},",
            "    booktitle = {${3:booktitle}},",
            "    publisher = {${4:publisher}},",
            "    editor = {${5:?_editor}},",
            "    volume = {${6:?_volume}},",
            "    number = {${7:?_number}},",
            "    series = {${8:?_series}},",
            "    type = {${9:?_type}},",
            "    ?_chapter = {${10:?_chapter}},",
            "    pages = {${11:?_pages}},",
            "    ?_address = {${12:?_address}},",
            "    ?_edition = {${13:?_edition}},",
            "    year = {${14:year}},",
            "    month = {${15:?_month}},",
            "    note = {${16:?_note}},",
            "}"
        ]
    },
    "@inproceedings": {
        "prefix": "@inproceedings",
        "description": "An article in a conference proceedings.",
        "body": [
            "@inproceedings{Xinproceedings,",
            "    author = {${1:author}},",
            "    title = {${2:title}},",
            "    booktitle = {${3:booktitle}},",
            "    editor = {${4:?_editor}},",
            "    volume = {${5:?_volume}},",
            "    number = {${6:?_number}},",
            "    series = {${7:?_series}},",
            "    pages = {${8:?_pages}},",
            "    address = {${9:?_address}},",
            "    organization = {${10:?_organization}},",
            "    publisher = {${11:?_publisher}},",
            "    year = {${12:year}},",
            "    month = {${13:?_month}},",
            "    note = {${14:?_note}},",
            "}"
        ]
    },
    "@manual": {
        "prefix": "@manual",
        "description": "Technical manual",
        "body": [
            "@manual{Xmanual,",
            "    title = {${1:title}},",
            "    author = {${2:?_author}},",
            "    organization = {${3:?_organization}},",
            "    address = {${4:?_address}},",
            "    edition = {${5:?_edition}},",
            "    year = {${6:year}},",
            "    month = {${7:?_month}},",
            "    note = {${8:?_note}},",
            "}"
        ]
    },
    "@mastersthesis": {
        "prefix": "@mastersthesis",
        "description": "Master's thesis",
        "body": [
            "@mastersthesis{Xthesis,",
            "    author = {${1:author}},",
            "    title = {${2:title}},",
            "    school = {${3:school}},",
            "    type = {${4:?_type}},",
            "    address = {${5:?_address}},",
            "    year = {${6:year}},",
            "    month = {${7:?_month}},",
            "    note = {${8:?_note}},",
            "}"
        ]
    },
    "@misc": {
        "prefix": "@misc",
        "description": "Template useful for other kinds of publication",
        "body": [
            "@misc{Xmisc,",
            "    author = {${1:?_author}},",
            "    title = {${2:?_title}},",
            "    howpublished = {${3:?_howpublished}},",
            "    year = {${4:?_year}},",
            "    month = {${5:?_month}},",
            "    note = {${6:?_note}},",
            "}"
        ]
    },
    "@phdthesis": {
        "prefix": "@phdthesis",
        "description": "Ph.D. thesis",
          "body": [
              "@phdthesis{Xphdthesis,",
              "    author = {${1:author}},",
              "    title = {${2:title}},",
              "    school = {${3:school}},",
              "    address = {${4:?_address}},",
              "    year = {${5:year}},",
              "    month = {${6:?_month}},",
              "    keywords = {${7:?_keywords}},",
              "    note = {${8:?_note}},",
              "}"
        ]
    },
    "@proceedings": {
        "prefix": "@proceedings",
        "description": "The proceedings of a conference.",
        "body": [
            "@proceedings{Xproceedings,",
            "    title = {${1:title}},",
            "    editor = {${2:?_editor}},",
            "    volume = {${3:?_volume}},",
            "    number = {${4:?_number}},",
            "    series = {${5:?_series}},",
            "    address = {${6:?_address}},",
            "    organization = {${7:?_organization}},",
            "    publisher = {${8:?_publisher}},",
            "    year = {${9:year}},",
            "    month = {${10:?_month}},",
            "    note = {${11:?_note}},",
            "}"
        ]
    },
    "@techreport": {
        "prefix": "@techreport",
        "description": "Technical report from educational, commercial or standardization institution.",
        "body": [
            "@techreport{Xtreport,",
            "    author = {${1:author}},",
            "    title = {${2:title}},",
            "    institution = {${3:institution}},",
            "    type = {${4:?_type}},",
            "    number = {${5:?_number}},",
            "    address = {${6:?_address}},",
            "    year = {${7:year}},",
            "    month = {${8:?_month}},",
            "    note = {${9:?_note}},",
            "}"
        ]
    },
    "@unpublished": {
        "prefix": "@unpublished",
        "description": "An unpublished article, book, thesis, etc.",
        "body": [
            "@unpublished{Xunpublished,",
            "    author = {${1:author}},",
            "    title = {${2:title}},",
            "    year = {${3:?_year}},",
            "    month = {${4:?_month}},",
            "    note = {${5:note}},",
            "}"
        ]
    }
}
