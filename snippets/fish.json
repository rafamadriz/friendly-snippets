{
  "shebang": {
    "prefix": "shebang",
    "description": "shebang",
    "body": "#!/usr/bin/env fish"
  },
  "if operator": {
    "prefix": "if",
    "description": "if operator",
    "body": [
      "if ${1:condition}",
      "\t$0",
      "end"
    ]
  },
  "if else operator": {
    "prefix": "if-else",
    "description": "if operator",
    "body": [
      "if ${1:condition}",
      "\t${2:echo}",
      "else",
      "\t$0",
      "end"
    ]
  },
  "else operator": {
    "prefix": "else",
    "description": "else operator",
    "body": [
      "else",
      "\t$0"
    ]
  },
  "if compare with comparison operator": {
    "prefix": "if-compare",
    "description": "if operator",
    "body": [
      "if test ${1:first} ${2|=,!=,-eq,-ne,-gt,-ge,-lt,-le,-nt,-ot,-ef|} ${3:second}",
      "\t$0",
      "end"
    ]
  },
  "if else with comparison operator": {
    "prefix": "if-compare-else",
    "description": "if else operator",
    "body": [
      "if test ${1:first} ${2|=,!=,-eq,-ne,-gt,-ge,-lt,-le,-nt,-ot,-ef|} ${3:second}",
      "\t${4:echo}",
      "else",
      "\t$0",
      "end"
    ]
  },
  "while operator": {
