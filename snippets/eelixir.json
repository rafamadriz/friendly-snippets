{
  "%": {
    "prefix": "%",
    "body": "<% $0 %>"
  },
  "=": {
    "prefix": "=",
    "body": "<%= $0 %>"
  },
  "gettext": {
    "prefix": "gt",
    "body": "<%= gettext(\"$0\") %>"
  },
  "for": {
    "prefix": "for",
    "body": [
      "<%= for ${1:item} <- ${2:items} do %>",
      "  $0",
      "<% end %>"
    ]
  },
  "if": {
    "prefix": "if",
    "body": [
      "<%= if ${1} do %>",
      "  $0",
      "<% end %>"
    ]
  },
  "ife": {
    "prefix": "if",
    "body": [
      "<%= if ${1} do %>",
      "  $2",
      "<% else %>",
      "  $0",
      "<% end %>"
    ]
  },
  "lin": {
    "prefix": "if",
    "body": "<%= link \"${1:Submit}\", to: ${2:\"/users\"}, method: ${3::delete} %>"
  }
}
