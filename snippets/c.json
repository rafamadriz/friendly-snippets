  },
  "#define macro()": {
    "prefix": "#deff",
    "body": ["#define ${1:MACRO}($2) ($3)"],
    "description": "Code snippet for a func-like macro"
  },
  "#if": {
    "prefix": "#if",
    "body": [
      "#if ${1:0}",
