{
	"info": {
    "prefix": "inf",
    "body": [
			"-----------------------------------",
			"\tFile         : ${TM_FILENAME}",
			"\tDescription  : ${1:Description}",
			"\tAuthor       : Kevin",
			"\tLast Modified: 09 Jan 2023, 15:29
			"-----------------------------------\n",
			"$0"
    ],
    "description": "file main info"
  },
	"file-info_yaml": {
		"prefix": "infYaml",
		"body": [
			"---",
			"Title         : ${TM_FILENAME}",
			"Author        : Kevin",
			"Date          : ${CURRENT_DATE}/${CURRENT_MONTH}/${CURRENT_YEAR}, ${CURRENT_HOUR}:${CURRENT_MINUTE}",
			"Abstract      : ${2:types}",
			"output        : ",
			"\tpdf_document: default",
			"\thtml_notebook: default",
			"---",
      "$0"
		],
		"description": "file info in yaml for rmd files"
	},
	"Date": {
    "prefix": "date",
    "body": [
      "${CURRENT_DATE}/${CURRENT_MONTH}/${CURRENT_YEAR}"
    ],
    "description": ""
  },
	"Date&Time": {
    "prefix": "datetime",
    "body": [
      "${CURRENT_DATE}/${CURRENT_MONTH}/${CURRENT_YEAR} - ${CURRENT_HOUR}:${CURRENT_MINUTE}"
    ],
    "description": "Date and time stamp"
  },
  "Time": {
    "prefix": "time",
    "body": [
      "${CURRENT_HOUR}:${CURRENT_MINUTE}"
    ],
    "description": "Timestamp"
  },
	"TOC": {
		"prefix": "toc",
		"body": [
			"# TOC",
			"- ${1}",
			"  - ${2}"
		],
		"description": "Table of Contents"
	}
}
