{
  "tailwind-base": {
    "prefix": "tailwindBase",
    "body": [
      "@tailwind base;",
      "@tailwind components;",
      "@tailwind utilities;",
      "$0"
    ],
    "description": "An opinionated set of base styles for Tailwind projects."
  },
  "container": {
    "prefix": "container",
    "body": [
      "container $0"
    ],
    "description": "container => sm(640px), md(768px), lg(1024px), xl(1280px), 2xl(1536px) and use max-width overall"
  },
  "aspect-auto": {
    "prefix": "aspectAuto",
    "body": [
      "aspect-auto $0"
    ],
    "description": "aspect-ratio: auto"
  },
  "aspect-square": {
    "prefix": "aspectSquare",
    "body": [
      "aspect-square $0"
    ],
    "description": "aspect-ratio: 1 / 1;"
  },
  "aspect-video": {
    "prefix": "aspectVideo",
    "body": [
      "aspect-video $0"
    ],
    "description": "aspect-ratio: 16 / 9;"
  },
  "aspect-arbitrary": {
    "prefix": "aspectArbitrary",
    "body": [
      "aspect-[$1] $0"
    ],
    "description": "aspect arbitrary values"
  },
  "columns": {
    "prefix": "columnsNumber",
    "body": [
      "${1|columns-1,columns-2,columns-3,columns-4,columns-5,columns-6,columns-7,columns-8,columns-9,columns-10,columns-11,columns-12|} $0"
    ],
    "description": "Property => columns: 1, columns: 2, columns: 3, columns: 4, columns: 5, columns: 6, columns: 7, columns: 8, columns: 9, columns: 10, columns: 11, columns: 12"
  },
  "columns-auto": {
    "prefix": "columnsAuto",
    "body": [
      "columns-auto $0"
    ],
    "description": "columns: auto"
  },
  "columns-3xs": {
    "prefix": "columns3xs",
    "body": [
      "columns-3xs $0"
    ],
    "description": "columns: 16rem; /* 256px */"
  },
  "columns-2xs": {
    "prefix": "columns2xs",
    "body": [
      "columns-2xs $0"
    ],
    "description": "columns: 18rem; /* 288px */"
  },
  "columns-xs": {
    "prefix": "columnsXS",
    "body": [
      "columns-xs $0"
    ],
    "description": "columns: 20rem; /* 320px */"
  },
  "columns-sm": {
